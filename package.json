{
  "name": "nlf-code",
  "displayName": "NLF Code - A Syntax Highlighter for nlFilters",
  "description": "This extension provides syntax highlighting for nlFilters files which is used in NicoCache_nl software, the local proxy software for NicoNico.",
  "version": "1.0.1",
  "publisher": "roflsunriz",
  "engines": {
    "vscode": "^1.99.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roflsunriz/nlFilters-Syntax-HighLighter.git"
  },
  "bugs": {
    "url": "https://github.com/roflsunriz/nlFilters-Syntax-HighLighter/issues"
  },
  "homepage": "https://github.com/roflsunriz/nlFilters-Syntax-HighLighter#readme",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "nlFilters",
    "syntax",
    "highlighting"
  ],
  "contributes": {
    "languages": [{
        "id": "nlfilters",
        "aliases": ["NLFilters", "nlFilters"],
        "extensions": [".txt"],
        "configuration": "./language-configuration.json"
    }],
    "grammars": [{
        "language": "nlfilters",
        "scopeName": "source.nlfilters",
        "path": "./syntaxes/text.tmLanguage.json",
        "embeddedLanguages": {
            "source.js": "javascript",
            "source.css": "css",
            "meta.embedded.block.html": "html"
        }
    }]
  }
}
